#include<iostream>
#include<cmath>
using namespace std;

void fun(int a,int b,int c)
{
	int d=b*b-4*a*c;
	if(d>0)
	{
		cout<<"Real and distinct root, d= "<<d;
	}
	if(d==0)
	{
		cout<<"Real and equal root, d= "<<d;
	}
	if(d<0)
	{
		cout<<"Imaginary roots, d= "<<d;
	}
}
int main()
{
	int a,b,c;
	cout<<"Enter a,b,c ";
	cin>>a>>b>>c;
	fun(a,b,c);
}
