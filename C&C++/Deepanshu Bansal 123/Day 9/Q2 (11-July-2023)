#include<iostream>
using namespace std;

void fun(int array[],int n)
{
	for(int i=0;i<n;i++)
	{
		if(array[i]<0)
		{
			cout<<array[i]<<" ";
		}
	}
	for(int i=0;i<n;i++)
	{
		if(array[i]>=0)
		{
			cout<<array[i]<<" ";
		}
	}
}
int main()
{
	int array[100],i,n;
	cout<<"Enter n number of array elements ";
	cin>>n;
	for(i=0;i<n;i++)
	{
		cout<<"Enter array element ";
		cin>>array[i];
	}
	fun(array,n);
}               
