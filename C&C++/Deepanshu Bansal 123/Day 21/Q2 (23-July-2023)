#include<iostream>
#include<algorithm>
using namespace std;

void fun(int n,int array[])
{
	int sum=0;
	for(int i=0;i<n;i++)
	{
		sum=sum+array[i];
	}
	cout<<"Sum of an array elements "<<sum;
}

int main()
{
    int n,array[100];
    cout<<"Enter n number of elements in array ";
    cin>>n;
    for(int i=0;i<n;i++)
    {
    	cout<<"Enter array ";
    	cin>>array[i];
	}
    fun(n,array);
}
