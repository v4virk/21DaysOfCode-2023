#include <stdio.h>
int main()
{
    int year;
    printf("Enter a year if it's a leap year or not :");
    scanf("%d" , &year);
    if(year%4==0 && year%400==0)
    {
        printf("leap year");
    }
    else if(year%100==0)
    {
             printf("not leap year");
    }
    else
    {
        printf("");
    }
    return 0;
}
