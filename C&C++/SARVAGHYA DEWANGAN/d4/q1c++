#include<iostream>
using namespace std;

int main()
{
	int a,b,c;
	cout<<"enter the sides: "<<endl;
	cin>>a>>b>>c;
	cout<<"the sides are : "<<a<<" "<<b<<" "<<c<<endl;
	
	if((a==b)||(b==c)||(a==c))
	{
		if((a!=c)||(b!=a)||(b!=c))
		{
			cout<<"isoscales";
		}
		else
		{
			cout<<"equilateral";
		}
	}
	else
	{
		cout<<"scalene";
	}
}
