                                                 QUE 1
#include<stdio.h>
int main()
{
    int n1=0;
    int n2=1;
    int n3;
    int num;
    printf("enter the no upto which u want to print foebonacci series:\n");
    scanf("%d",&num);
    printf("fibonacci series:\n");
    printf("%d %d",n1,n2);
    for (int i=0;i<num;i++)
    {
        n3=n1+n2;
        n1=n2;
        n2=n3;
        printf(" %d",n3);
    }
    return 0;
}

                                                    QUE 2
#include<stdio.h>
int main() 
{
    int a, b, max;
    printf("Enter the two numbers : ");
    scanf("%d %d", &a, &b);
    max = (a > b) ? a : b; 
    while (max % a != 0 || max % b != 0) 
    {
            max++;
    }
    printf("The LCM of %d and %d is %d.", a, b, max);
    return 0;
}
